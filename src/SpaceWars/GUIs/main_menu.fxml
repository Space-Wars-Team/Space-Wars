                <Button fx:id="playbutton" layoutX="159.0" layoutY="200.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="83.0" text="Play" />
                <ImageView fitHeight="130.0" fitWidth="200.0" layoutX="100.0" layoutY="36.0">
                    <image>
                        <Image url="@/images/spacewars.jpg" />
