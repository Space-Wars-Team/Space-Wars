                <Button fx:id="subbutton" layoutX="159.0" layoutY="343.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="83.0" text="Subscription" />
                <Button fx:id="qbutton" layoutX="159.0" layoutY="390.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="83.0" text="Quit Game" />
